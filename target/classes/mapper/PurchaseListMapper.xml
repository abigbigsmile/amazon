<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.smile.amazon.mapper.PurchaseListMapper">
  <resultMap id="BaseResultMap" type="com.smile.amazon.model.PurchaseList">
    <result column="book_id" jdbcType="INTEGER" property="bookId" />
    <result column="cart_id" jdbcType="INTEGER" property="cartId" />
    <result column="book_price" jdbcType="DOUBLE" property="bookPrice" />
    <result column="book_num" jdbcType="TINYINT" property="bookNum" />
  </resultMap>
  <insert id="insert" parameterType="com.smile.amazon.model.PurchaseList">
    insert into purchase_list (book_id, cart_id, book_price, 
      book_num)
    values (#{bookId,jdbcType=INTEGER}, #{cartId,jdbcType=INTEGER}, #{bookPrice,jdbcType=DOUBLE}, 
      #{bookNum,jdbcType=TINYINT})
  </insert>
  <insert id="insertSelective" parameterType="com.smile.amazon.model.PurchaseList">
    insert into purchase_list
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="bookId != null">
        book_id,
      </if>
      <if test="cartId != null">
        cart_id,
      </if>
      <if test="bookPrice != null">
        book_price,
      </if>
      <if test="bookNum != null">
        book_num,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides=",">
      <if test="bookId != null">
        #{bookId,jdbcType=INTEGER},
      </if>
      <if test="cartId != null">
        #{cartId,jdbcType=INTEGER},
      </if>
      <if test="bookPrice != null">
        #{bookPrice,jdbcType=DOUBLE},
      </if>
      <if test="bookNum != null">
        #{bookNum,jdbcType=TINYINT},
      </if>
    </trim>
  </insert>
</mapper>